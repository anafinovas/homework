public class Person {

String name;
int age;

public String getName() {
    return name;
}
public void setName(String name)  {
    this.name = name;
}
public int getAge() {
    return age;
}
public void setAge(int age) {
    if (age >= 0) { 
        this.age = age;
    }
}

public static void main(String[] args) {
    Person person = new Person();
    person.setAge(5);
    System.out.println(person.getAge());
}
}
